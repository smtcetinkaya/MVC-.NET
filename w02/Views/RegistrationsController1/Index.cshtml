@model List<Student>
<h1> Students Registration Operation</h1>

<h3>Student List</h3>

<a href="/RegistrationsController1/Create"> Create New</a>

<table class="table">
	<tr>
		<th>Id</th>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Department</th>
		<th>About</th>
	</tr>
	@{
		foreach (var student in Model)
		{
			<tr>
				<th>@(student.Id)</th>
				<th>@(student.FirstName)</th>
				<th>@(student.LastName)</th>
				<th>@(student.Department)</th>
				<td>
					<a href="/RegistrationsController1/About?id=@(student.Id)">About Me</a> &nbsp;
					<a href="/RegistrationsController1/edit?id=@(student.Id)">Edit</a> &nbsp;
					<a href="/RegistrationsController1/Delete?id=@(student.Id)">Delete</a>
				</td>
			</tr>
		}
	}
</table>